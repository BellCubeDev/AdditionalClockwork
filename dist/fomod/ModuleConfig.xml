<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://qconsulting.ca/fo3/ModConfig5.0.xsd">
  <moduleName position="Left" colour="000000">Additional Clockwork</moduleName>
  <moduleImage path="fomod\images\AdditionalClockwork.png" showImage="true" showFade="true" height="-1"/>
  <installSteps order="Explicit">
    <installStep name="Additional Clockwork">
      <optionalFileGroups order="Explicit">
        <group name="Description" type="SelectAll">
          <plugins order="Explicit">
            <plugin name="Description">
              <description>Additional Clockwork is a suite of fully-voiced and lore-friendly Clockwork changes and tweaks intended to fit the origional feel of Clockwork. In addition, I've also made patches to acomadate changes made by other mods that, either intentionally or unintentionally, have an impact on Clockwork.</description>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
        <group name="Perspective" type="SelectExactlyOne">
          <plugins order="Explicit">
            <plugin name="Second Person">
              <description>All texts are phrased in Second Person, as they are by default in Skyrim.</description>
              <conditionFlags>
                <flag name="Perspective">2</flag>
              </conditionFlags>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
            <plugin name="First Person">
              <description>All messages and texts are phrased in the First Person perspective. Useful when you're using mods like Your Own Thoughts.</description>
              <image path="fomod\images\YOT.png"/>
              <conditionFlags>
                <flag name="Perspective">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
        <group name="Reccomendations" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Author's Reccomendations">
              <description>Applies BellCube's recommendations to the installer. You still have to go through each step, but my personal preferences are already selected.&#13;

(Does NOT include Integration, Patches, &amp;amp; Consistency modules)</description>
              <image path="fomod\images\CubicBell-256.png"/>
              <conditionFlags>
                <flag name="RecAuthor">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
            <plugin name="Testing">
              <description>Select everything in-testing</description>
              <conditionFlags>
                <flag name="Testing">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
      </optionalFileGroups>
    </installStep>
    <installStep name="Just Clockwork">
      <optionalFileGroups order="Explicit">
        <group name="Courier Counterspell" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Courier Counterspell">
              <description>Uses a script to disable World Interaction events (aka Random Encounters) when at Clockwork Castle, which primarily fixes the courier issue. No more monomaniacal correspondence-wizard. Rain or shine, GET OFF MY LAWN!</description>
              <image path="fomod\images\CourierCounterspell.png"/>
              <conditionFlags>
                <flag name="CourierCounterspell">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="RecAuthor" value="1"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
        <group name="Marked Shadows" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Termini">
              <description>Adds a shadowmark easily visible in each of the terminus locations.</description>
              <image path="fomod\images\ShadowmarksRiftenTerminus.png"/>
              <conditionFlags>
                <flag name="ShadowmarksTermini">1</flag>
                <flag name="Shadowmarks">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="ShadowmarksMerged" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Travel Room">
              <description>Adds a shadowmark inside the Travel Room beside the stairs (door to the Main Hall).</description>
              <image path="fomod\images\ShadowmarksTravelRoom.png"/>
              <conditionFlags>
                <flag name="ShadowmarksTravelRoom">1</flag>
                <flag name="Shadowmarks">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="ShadowmarksMerged" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Castle Exterior">
              <description>Adds a Guild Member shadowmark beside Clockwork Castle's front door.</description>
              <image path="fomod\images\ShadowmarksCastleExt.png"/>
              <conditionFlags>
                <flag name="ShadowmarksCastleExt">1</flag>
                <flag name="Shadowmarks">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="ShadowmarksMerged" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Merged">
              <description>Merges all three options into one plugin.</description>
              <image path="fomod\images\ShadowmarksMerged.png"/>
              <conditionFlags>
                <flag name="ShadowmarksMerged">1</flag>
                <flag name="Shadowmarks">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="RecAuthor" value="1"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
        <group name="Interesting Inhabitants" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Interesting Inhabitants">
              <description>By far the biggest module yet! Makes the castle inhabitants seem much more life-like!</description>
              <conditionFlags>
                <flag name="InterestingInhabitants">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="Testing" value="1"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
        <group name="Woodworker's Whim" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Script - Enabled Globally">
              <description>Adds a script for the player to search for a nearby axe in the worldspace (i.e. not in a container) if they don't have one in their inventory.&#13;

This module applies the script to EVERY instance of the vanilla Wood Chopping Block (even ones added by mods).</description>
              <image path="fomod\images\WoodworkersWhimScript.png"/>
              <conditionFlags>
                <flag name="WoodWhimScriptLevel">2</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="WoodWhimScriptLevel" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Script - Work Room Only">
              <description>Adds a script for the player to search for a nearby axe in the worldspace (i.e. not in a container) if they don't have one in their inventory.&#13;

This module applies the script ONLY to the Wood Chopping Block in Clockwork Castle's Work Room.</description>
              <image path="fomod\images\WoodworkersWhimScript.png"/>
              <conditionFlags>
                <flag name="WoodWhimScriptLevel">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="WoodWhimScriptLevel" value="2"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="RecAuthor" value="1"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Work Room Axe Mount">
              <description>Adds an Axe Mount to Clockwork Castle's Work Room, just above the Wood Chopping Block.</description>
              <image path="fomod\images\WoodworkersWhimMount.png"/>
              <conditionFlags>
                <flag name="WoodWhimMount">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="RecAuthor" value="1"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
        <group name="Minor Fixes and Changes" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Mausoleum Dummy Door Fix">
              <description>When speaking with Lamashtu after returning the first time from Nurndural, a fake door is used instead of the real door. to stop you from leaving before the scene is completed. However, if you leave dialogue before Lamashtu finishes her last line, you can EASILY watch as the door pops out of existance... BECAUSE IT IS ONE WHOLE DOOR IN FRONT OF THE OTHER! With this installed, the door has no noticeable pop-out.</description>
              <image path="fomod\images\SmallFixMausoleumDoor.png"/>
              <conditionFlags>
                <flag name="Small_DDoorFix">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="RecAuthor" value="1"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Clockwork Title Remover">
              <description>Thanks to Foamimi for the idea!&#13;

Removes the floating Clockwork text above Clockwork Castle when you first enter the valley.</description>
              <image path="fomod\images\ClockworkTitleRemover.png"/>
              <conditionFlags>
                <flag name="Small_NoTitle">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
      </optionalFileGroups>
    </installStep>
    <installStep name="Integration, Patches, &amp; Consistency">
      <optionalFileGroups order="Explicit">
        <group name="High Poly Project" type="SelectAny">
          <plugins order="Explicit">
            <plugin name=" Extra Coals Fix">
              <description>Uses High Poly Project's mesh for the Work Room's extra smelter coals. Because this mesh is used elsewhere (in the Velothi Mountain Tunnels and in Nurndural), the plugin also makes changes there.</description>
              <image path="fomod\images\HPP_CoalsFix.png"/>
              <conditionFlags>
                <flag name="IPC_HPP_Coals">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="IPC_HPP_Merged" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Hay Z-Fight Fix">
              <description>Fixes z-fighting that occurs in various points around the castle and in some Terminus locations by raising or lowering the hay piles' meshes.</description>
              <image path="fomod\images\HPP_HayFix.png"/>
              <conditionFlags>
                <flag name="IPC_HPP_Hay">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="IPC_HPP_Merged" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Merged">
              <description>Merges all of the High Poly Project options into one plugin.</description>
              <image path="fomod\images\HighPolyProject.png"/>
              <conditionFlags>
                <flag name="IPC_HPP_Merged">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
      </optionalFileGroups>
    </installStep>
    <installStep name="Sorting">
      <optionalFileGroups order="Explicit">
        <group name="Ingredients" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Complete Cooking and Alchemy Overhaul">
              <description>Adds CACO's items to Clockwork's alchemy list.</description>
              <image path="fomod\images\CACO.jpg"/>
              <conditionFlags>
                <flag name="Sorting_Ing_CACO">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <fileDependency file="Complete Alchemy &amp; Cooking Overhaul.esp" state="Active"/>
                        <dependencies operator="Or">
                          <fileDependency file="ccbgssse037-curios.esl" state="Inactive"/>
                          <fileDependency file="ccbgssse037-curios.esl" state="Missing"/>
                        </dependencies>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="Sorting_Ing_ccCuriosCACO" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Rare Curios">
              <description>Adds Rare Curios' items to Clockwork's alchemy list.</description>
              <image path="fomod\images\RareCurios.jpg"/>
              <conditionFlags>
                <flag name="Sorting_Ing_ccCurios">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <fileDependency file="ccbgssse037-curios.esl" state="Active"/>
                        <dependencies operator="Or">
                          <fileDependency file="Complete Alchemy &amp; Cooking Overhaul.esp" state="Inactive"/>
                          <fileDependency file="Complete Alchemy &amp; Cooking Overhaul.esp" state="Missing"/>
                        </dependencies>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                    <pattern>
                      <dependencies operator="And">
                        <flagDependency flag="Sorting_Ing_ccCuriosCACO" value="1"/>
                      </dependencies>
                      <type name="NotUsable"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
            <plugin name="Rare Curios + CACO">
              <description>Adds CACO's AND Rare Curios' items to Clockwork's alchemy list.</description>
              <image path="fomod\images\CACO_ccCurios.png"/>
              <conditionFlags>
                <flag name="Sorting_Ing_ccCuriosCACO">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <fileDependency file="Complete Alchemy &amp; Cooking Overhaul.esp" state="Active"/>
                        <fileDependency file="ccbgssse037-curios.esl" state="Active"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
        <group name="Food" type="SelectAny">
          <plugins order="Explicit">
            <plugin name="Complete Cooking and Alchemy Overhaul">
              <description>Adds CACO's items to Clockwork's Kitchen lists.</description>
              <image path="fomod\images\CACO.jpg"/>
              <conditionFlags>
                <flag name="Sorting_Food_CACO">1</flag>
              </conditionFlags>
              <typeDescriptor>
                <dependencyType>
                  <defaultType name="Optional"/>
                  <patterns>
                    <pattern>
                      <dependencies operator="And">
                        <fileDependency file="Complete Alchemy &amp; Cooking Overhaul.esp" state="Active"/>
                      </dependencies>
                      <type name="Recommended"/>
                    </pattern>
                  </patterns>
                </dependencyType>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
      </optionalFileGroups>
    </installStep>
  </installSteps>
  <conditionalFileInstalls>
    <patterns>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="WoodWhimMount" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Normal\WoodworkersWhim\ClockworkWoodWhimWWMount.esp" destination="ClockworkWoodWhimWWMount.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="WoodWhimScriptLevel" value="1"/>
        </dependencies>
        <files>
          <folder source="Modules\Normal\WoodworkersWhim\WorkRoom" destination="" priority="0" alwaysInstall="false" installIfUsable="false"/>
          <folder source="Modules\Normal\WoodworkersWhim\ScriptAndSource" destination="" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="WoodWhimScriptLevel" value="2"/>
        </dependencies>
        <files>
          <folder source="Modules\Normal\WoodworkersWhim\Global" destination="" priority="0" alwaysInstall="false" installIfUsable="false"/>
          <folder source="Modules\Normal\WoodworkersWhim\ScriptAndSource" destination="" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="WoodWhimScriptLevel" value="2"/>
          <flagDependency flag="Perspective" value="1"/>
        </dependencies>
        <files>
          <folder source="translations\englsish-1p\WoodWhimGlobal" destination="Strings" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="WoodWhimScriptLevel" value="1"/>
          <flagDependency flag="Perspective" value="1"/>
        </dependencies>
        <files>
          <folder source="translations\englsish-1p\WoodWhimWW" destination="Strings" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="WoodWhimScriptLevel" value="2"/>
          <flagDependency flag="Perspective" value="2"/>
        </dependencies>
        <files>
          <folder source="translations\englsish-1p\WoodWhimGlobal" destination="Strings" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="WoodWhimScriptLevel" value="1"/>
          <flagDependency flag="Perspective" value="2"/>
        </dependencies>
        <files>
          <folder source="translations\englsish-1p\WoodWhimWW" destination="Strings" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="IPC_HPP_Merged" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\IntegrationPatchesNConsistency\HighPolyProject\meshes\clockwork\clutter\common\smeltercoal01.nif" destination="meshes\clockwork\clutter\common\smeltercoal01.nif" priority="0" alwaysInstall="false" installIfUsable="false"/>
          <file source="Modules\IntegrationPatchesNConsistency\HighPolyProject\ClockworkHPPMergedFixes.esp" destination="ClockworkHPPMergedFixes.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="IPC_HPP_Coals" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\IntegrationPatchesNConsistency\HighPolyProject\ClockworkHPPCoalsFix.esp" destination="ClockworkHPPCoalsFix.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
          <file source="Modules\IntegrationPatchesNConsistency\HighPolyProject\meshes\clockwork\clutter\common\smeltercoal01.nif" destination="meshes\clockwork\clutter\common\smeltercoal01.nif" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="IPC_HPP_Hay" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\IntegrationPatchesNConsistency\HighPolyProject\ClockworkHPPHayFix.esp" destination="ClockworkHPPHayFix.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="CourierCounterspell" value="1"/>
        </dependencies>
        <files>
          <folder source="Modules\Normal\CourierCounterspell" destination="" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="ShadowmarksTravelRoom" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Normal\Shadowmarks\ClockworkShadowmarksTravelRoom.esp" destination="ClockworkShadowmarksTravelRoom.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="ShadowmarksTermini" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Normal\Shadowmarks\ClockworkShadowmarksTermini.esp" destination="ClockworkShadowmarksTermini.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="ShadowmarksCastleExt" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Normal\Shadowmarks\ClockworkShadowmarksCastleExt.esp" destination="ClockworkShadowmarksCastleExt.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="ShadowmarksMerged" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Normal\Shadowmarks\ClockworkShadowmarksMerged.esp" destination="ClockworkShadowmarksMerged.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="Shadowmarks" value="1"/>
        </dependencies>
        <files>
          <folder source="Modules\Normal\Shadowmarks\Scripts" destination="" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="Sorting_Ing_ccCurios" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Sorting\Ingredients\ClockworkSorting_Ing_CCCurios.esp" destination="ClockworkSorting_Ing_CCCurios.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="Sorting_Ing_CACO" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Sorting\Ingredients\ClockworkSorting_Ing_CACO.esp" destination="ClockworkSorting_Ing_CACO.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="Sorting_Ing_ccCuriosCACO" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Sorting\Ingredients\ClockworkSorting_Ing_CCCurios_CACO.esp" destination="ClockworkSorting_Ing_CCCurios_CACO.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="Sorting_Food_CACO" value="1"/>
        </dependencies>
        <files>
          <file source="Modules\Sorting\Food\ClockworkSorting_Food_CACO.esp" destination="ClockworkSorting_Food_CACO.esp" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
      <pattern>
        <dependencies operator="And">
          <flagDependency flag="InterestingInhabitants" value="1"/>
        </dependencies>
        <files>
          <folder source="Modules\Normal\InterestingInhabitants" destination="" priority="0" alwaysInstall="false" installIfUsable="false"/>
        </files>
      </pattern>
    </patterns>
  </conditionalFileInstalls>
</config>
